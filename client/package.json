  "scripts": {
    "build": "sh -c 'export NODE_OPTIONS=\"$NODE_OPTIONS --max_old_space_size=4096\" && yarn nearley && webpack --mode production --config webpack.prod.cjs'",
    "build-dev": "sh -c 'export NODE_OPTIONS=\"$NODE_OPTIONS --max_old_space_size=4096\" && yarn nearley && webpack --mode development --config webpack.dev.cjs'",
    "nearley": "sh nearley/helper.sh",
    "start": "yarn webpack-dev-server",
    "webpack-dev-server": "yarn nearley && webpack serve --config webpack.dev.cjs"
  },
