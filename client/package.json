{
  "name": "@cubeartisan/client",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "type": "module",
  "browserslist": "> 2%, not dead",
  "sideEffects": [
    "**/pages/*.js",
    "**/*.css",
    "**/*.scss",
    "**/draftbots/*.js"
  ],
  "scripts": {
    "build": "yarn build:base --mode production --config webpack.prod.cjs",
    "build:base": "export NODE_OPTIONS=--max_old_space_size=4096 && yarn nearley && webpack",
    "build:dev": "yarn build:base --mode development --config webpack.dev.cjs",
    "lint": "eslint --quiet `yarn --silent list-files`",
    "lint:fix": "eslint --fix `yarn --silent list-files`",
    "lint:loud": "eslint `yarn --silent list-files`",
    "list-files": "git ls-files '*.js' '*.cjs' '*.mjs' '*.jsx'",
    "nearley": "sh nearley/helper.sh",
    "start": "yarn webpack-dev-server",
    "test": "yarn test:loud --silent",
    "test:loud": "jest --detectOpenHandles  --coverage",
    "test:watch": "yarn test:loud --watch --verbose false",
    "webpack-dev-server": "yarn nearley && webpack serve --config webpack.dev.cjs"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@cubeartisan/markdown": "^0.1.0",
    "@primer/octicons-react": "^11.0.0",
    "babel-loader": "^8.1.0",
    "chart.js": "^3.5.0",
    "classnames": "^2.2.6",
    "core-js": "^3.15.2",
    "core-js-pure": "^3.15.2",
    "html-to-text": "^5.1.1",
    "mtgdraftbots": "^1.3.3",
    "nearley": "^2.19.3",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-audio-player": "^0.17.0",
    "react-chartjs-2": "^3.0.3",
    "react-csv": "^2.0.3",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dnd-touch-backend": "^10.0.2",
    "react-dom": "17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-leaf-carousel": "^2.0.3",
    "react-pivottable": "^0.11.0",
    "react-player": "^2.6.1",
    "react-sortable-hoc": "^2.0.0",
    "react-tag-input": "^6.7.0",
    "react-tagcloud": "^2.1.1",
    "react-timeago": "^6.2.1",
    "reactstrap": "^8.9.0",
    "seedrandom": "^3.0.5",
    "shuffle-seed": "^1.1.6",
    "socket.io-client": "^4.1.3",
    "styled-components": "^5.3.0",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.42.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@react-mock/fetch": "^0.3.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@types/react": "^17.0.11",
    "@cubeartisan/client": "^0.1.0",
    "@cubeartisan/server": "^0.1.0",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^26.0.1",
    "browserslist": "^4.16.6",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "26.x.x",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jsdom": "^16.2.2",
    "prettier": "^2.3.0",
    "typescript": "^4.3.4",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "chokidar": "^3.4.0",
    "@jest/create-cache-key-function": "^27.0.1"
  }
}
