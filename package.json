{
  "name": "@cubeartisan/cubeartisan",
  "packageManager": "pnpm@7.16.1",
  "scripts": {
    "dev": "solid-start dev",
    "build": "solid-start build &&  gawk -i inplace '{sub(/string_decoder\\//, \"string_decoder\", $0)}1' dist/server.js",
    "start": "solid-start start",
    "test": "vitest run",
    "list-files": "git ls-files 'src/*.js' 'src/*.cjs' 'src/*.mjs' 'src/*.jsx' 'src/*.ts' 'src/*.tsx'",
    "lint": "sh -c 'eslint_d `pnpm run --silent list-files`'",
    "lint-fix": "sh -c 'eslint_d --fix `pnpm run --silent list-files`'",
    "lint-loud": "sh -c 'eslint_d `pnpm run --silent list-files`'",
    "cards": "echo 'No card database setup yet.",
    "checktypes": "tsc | loose-ts-check",
    "ci": "pnpm lint && pnpm checktypes",
    "error-files": "pnpm checktypes | grep 'src/' | cut -d. -f 1 | uniq | sort | gawk '{print \"\\\"\" $1 \"\\\".ts*\"}' | sed 's/\\([()]\\)/\\\\\\1/g' | xargs -I{} zsh -c \"echo {}\"\n"
  },
  "type": "module",
  "dependencies": {
    "@cubeartisan/carddb": "^1.2.0",
    "@solidjs/testing-library": "^0.7.0",
    "undici": "^5.21.2"
  },
  "devDependencies": {
    "@kobalte/core": "^0.8.2",
    "@radix-ui/colors": "github:radix-ui/colors",
    "@solid-primitives/media": "^2.1.6",
    "@solidjs/meta": "^0.28.4",
    "@solidjs/router": "^0.8.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/bad-words": "^3.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.37.0",
    "@types/lodash": "4.14.192",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.7",
    "@types/prettier": "^2.7.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@typescript-eslint/typescript-estree": "^5.57.1",
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/dynamic": "2.0.3",
    "@vanilla-extract/recipes": "0.4.0",
    "@vanilla-extract/sprinkles": "1.5.2",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "bad-words": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "clsx": "1.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-array-func": "^3.1.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "^0.12.0",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "eslint_d": "^12.2.1",
    "jsdom": "^21.1.1",
    "lodash": "4.17.21",
    "loose-ts-check": "^2.0.0",
    "mongoose": "^7.0.3",
    "nodemailer": "^6.9.1",
    "prettier": "^2.8.7",
    "solid-icons": "^1.0.4",
    "solid-js": "^1.7.3",
    "solid-start": "^0.2.26",
    "solid-start-node": "^0.2.26",
    "typescript": "<5.0.0",
    "utility-types": "3.10.0",
    "uuid": "^9.0.0",
    "vite": "^4.2.1",
    "vitest": "^0.30.0",
    "winston": "^3.8.2"
  }
}